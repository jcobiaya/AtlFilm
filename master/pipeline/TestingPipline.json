{
	"name": "TestingPipline",
	"properties": {
		"activities": [
			{
				"name": "Tickets",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:01.000",
						"requestMethod": "GET",
						"paginationRules": {
							"QueryParameters.{skip}": "RANGE:0:75000:1000"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"ID"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['event_bucket']"
								},
								"sink": {
									"name": "Event_Bucket",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['person']['id']"
								},
								"sink": {
									"name": "Person_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['person']['email']"
								},
								"sink": {
									"name": "Person_Email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['ticket_bucket']['id']"
								},
								"sink": {
									"name": "Ticket_Bucket.ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['ticket_bucket']['name']"
								},
								"sink": {
									"name": "Ticket_Bucket.Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['ticket_bucket']['price']"
								},
								"sink": {
									"name": "Ticket_Bucket.Price",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "['event']['id']"
								},
								"sink": {
									"name": "Event.ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['event']['name']"
								},
								"sink": {
									"name": "Event.Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['event']['venue']['id']"
								},
								"sink": {
									"name": "Event.Venue.ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['event']['venue']['name']"
								},
								"sink": {
									"name": "Event.Venue.Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['order']"
								},
								"sink": {
									"name": "Order_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['created_at']"
								},
								"sink": {
									"name": "Created_At",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['pass']['id']"
								},
								"sink": {
									"name": "Pass_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['used_at']"
								},
								"sink": {
									"name": "Used_At",
									"type": "DateTime"
								}
							}
						],
						"collectionReference": "$['tickets']"
					}
				},
				"inputs": [
					{
						"referenceName": "EventiveTickets",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlEventiveTickets",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}